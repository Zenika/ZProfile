{
  "name": "zprofile",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^10"
  },
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p 8080",
    "start:local": "next start -p 3000",
    "prettier": "prettier *.json *.js \"src/**/*.{js,jsx,ts,tsx}\" \".circleci/config.yml\"",
    "prettier:check": "npm run prettier -- --check",
    "prettier:write": "npm run prettier -- --write",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "ava",
    "test:integration": "npm run cypress:run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "dependencies": {
    "@apollo/client": "^3.3.9",
    "@auth0/auth0-react": "^1.2.0",
    "autoprefixer": "^10.2.4",
    "await-of": "^3.1.0",
    "graphql": "^15.5.0",
    "next": "^10.0.0",
    "next-pwa": "^5.0.5",
    "object-path": "^0.11.5",
    "postcss": "^8.2.6",
    "postcss-loader": "^4.1.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-swipeable": "^6.0.1",
    "tailwindcss": "^2.0.3",
    "use-debounce": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.14",
    "@types/object-path": "^0.11.0",
    "@types/react": "^17.0.0",
    "ava": "^3.15.0",
    "cypress": "^6.3.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
