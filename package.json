{
  "name": "zprofile",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=12"
  },
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p 8080",
    "start:local": "next start -p 3000",
    "prettier": "prettier *.json *.js \"src/**/*.{js,jsx,ts,tsx}\" \".circleci/config.yml\"",
    "prettier:check": "npm run prettier -- --check",
    "prettier:write": "npm run prettier -- --write",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "ava",
    "test:integration": "npm run cypress:run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "hasura": "hasura --project ./hasura",
    "seeds:generate": "ts-node ./hasura/seeds/seedscript.ts",
    "seeds:apply": "npm run hasura seeds apply"
  },
  "dependencies": {
    "@apollo/client": "^3.3.9",
    "@auth0/auth0-react": "^1.2.0",
    "await-of": "^3.1.0",
    "css-loader": "^4.3.0",
    "file-loader": "^6.2.0",
    "graphql": "^15.5.0",
    "next": "^10.0.0",
    "next-pwa": "^5.0.5",
    "object-path": "^0.11.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-responsive": "^8.2.0",
    "react-swipeable": "^6.0.1",
    "style-loader": "^1.2.1",
    "styled-components": "^5.3.0",
    "url-loader": "^4.1.1",
    "use-debounce": "^6.0.0"
  },
  "devDependencies": {
    "@types/faker": "^5.5.6",
    "@types/node": "^14.14.14",
    "@types/object-path": "^0.11.0",
    "@types/react": "^17.0.0",
    "autoprefixer": "^10.2.5",
    "ava": "^3.15.0",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "cypress": "^6.3.0",
    "faker": "^5.5.3",
    "hasura-cli": "^1.3.3",
    "json-sql": "^0.5.0",
    "postcss": "^8.2.14",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.1.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
