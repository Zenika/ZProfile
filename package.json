{
  "name": "zprofile",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run relay && next dev",
    "build": "npm run relay && next build",
    "start": "next start",
    "prettier": "prettier *.json \"src/**/*.{js,jsx,ts,tsx}\" \".circleci/config.yml\"",
    "prettier:check": "npm run prettier -- --check",
    "prettier:write": "npm run prettier -- --write",
    "relay": "relay-compiler --src ./ --exclude '**/.next/**' '**/node_modules/**' '**/test/**'  '**/__generated__/**' '**/hasura/**' '**/.circleci/**'  --schema ./schema.graphql",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "ava ",
    "test:integration": "npm run cypress:run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^1.0.0-beta.0",
    "autoprefixer": "^10.2.1",
    "next": "^10.0.0",
    "object-path": "^0.11.5",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.1.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-relay": "^10.1.2",
    "react-relay-network-modern": "^6.0.0",
    "react-relay-network-modern-ssr": "^1.4.0",
    "relay-hooks": "^4.0.0-rc.1",
    "relay-runtime": "^10.1.2",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/node": "^14.14.14",
    "@types/object-path": "^0.11.0",
    "@types/react": "^17.0.0",
    "@types/react-relay": "^7.0.17",
    "@types/relay-runtime": "^10.1.6",
    "ava": "^3.15.0",
    "babel-plugin-relay": "^10.1.2",
    "cypress": "^6.3.0",
    "graphql": "^15.4.0",
    "relay-compiler": "^10.1.2",
    "relay-compiler-language-typescript": "^13.0.2",
    "relay-config": "^10.1.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
